{
    "name": "julbel/api",
    "description": "A api poetry",
    "keywords": ["framework", "slim", "api"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.8",
        "slim/twig-view": "^2.2",
        "symfony/var-dumper": "^6.0",
        "vlucas/phpdotenv": "^2.4",
        "php-di/php-di": "^6.3",
        "guzzlehttp/guzzle": "^7.4",
        "predis/predis": "^1.1",
        "illuminate/pagination": "^8.77"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
